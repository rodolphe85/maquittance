<%= simple_form_for @invoice do |f| %>
  <%= f.input :title, label: 'Title' %>
  <%= f.input :date %>
  <%= f.input :from, placeholder: 'from' %>
  <%= f.input :to, placeholder: 'To' %>
  <%= f.input :content, inline_label: 'Yes' %>
  <%= f.association :property %>
  <%= f.association :tenant %>

  <%= f.button :submit %>
<% end %>
